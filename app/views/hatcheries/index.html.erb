<p id="notice"><%= notice %></p>

<h1>Listing Hatcheries</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hatcheries.each do |hatchery| %>
      <tr>
        <td><%= hatchery.name %></td>
        <td><%= link_to 'Show', hatchery %></td>
        <td><%= link_to 'Edit', edit_hatchery_path(hatchery) %></td>
        <td><%= link_to 'Destroy', hatchery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hatchery', new_hatchery_path %>
