<p id="notice"><%= notice %></p>

<h1>Listing Hatch Chicks</h1>

<table>
  <thead>
    <tr>
      <th>Hatch</th>
      <th>Breed</th>
      <th>Sex</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hatch_chicks.each do |hatch_chick| %>
      <tr>
        <td><%= hatch_chick.hatch %></td>
        <td><%= hatch_chick.breed %></td>
        <td><%= hatch_chick.sex %></td>
        <td><%= link_to 'Show', hatch_chick %></td>
        <td><%= link_to 'Edit', edit_hatch_chick_path(hatch_chick) %></td>
        <td><%= link_to 'Destroy', hatch_chick, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

